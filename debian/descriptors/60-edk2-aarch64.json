{
    "description": "UEFI firmware for aarch64",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "/usr/share/AAVMF/AAVMF_CODE.no-secboot.fd",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "/usr/share/AAVMF/AAVMF_VARS.fd",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "aarch64",
            "machines": [
                "virt-*"
            ]
        }
    ],
    "features": [
        "verbose-static"
    ],
    "tags": [

    ]
}
